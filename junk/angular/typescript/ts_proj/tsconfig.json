{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false
  },
  "exclude": [
    "node_modules",
    "dist/"
  ]
}

